PhysicalModel Features {
	
	PhysicalSystem FeaturesPhysicalSystem {
		NodeRef node1 : StandardNode
	}
	
	NodeClass StandardNode {
		runtime = RuntimeMultiThreaded
		priomin = -10
		priomax = 10
		
		DefaultThread PhysicalThread1 {
			execmode = mixed
			interval = 100ms
			prio = 0
			stacksize = 1024
			msgblocksize = 32
			msgpoolsize = 200
		}
	}

	RuntimeClass RuntimeMultiThreaded {
		model = multiThreaded
	} 
}
